{
  "manifest_version": 2,
  "name": "Convert Video to GIF",
  "short_name": "Chrome vidgif"
  "description": "",
  "version": "1.0",

  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "browser_action": {
    "default_title": "Imgur video to GIF"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "http://imgur.com/",
    "https://imgur.com/"
  ]
}
